	                android:id="@+id/DateHistoryLabel"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:gravity="center_vertical"
	                android:paddingLeft="@dimen/standardPadding"
	                android:paddingTop="@dimen/adjustmentPadding"
	                android:text="@string/dateLabel"
	                android:textAppearance="?android:attr/textAppearanceMedium"
	                android:textSize="@dimen/historyDetailsSize"
	                android:layout_weight="1" />
	
	            <TextView
	                android:id="@+id/DateHistoryValue"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_gravity="center_vertical"
	                android:layout_weight="1.08"
	                android:gravity="right"
	                android:paddingTop="@dimen/adjustmentPadding"
	                android:text="@string/defaultValue"
	                android:textAppearance="?android:attr/textAppearanceMedium"
	                android:textSize="@dimen/historyDetailsSize"
	                android:width="@dimen/numDisplayWidth" />
	

	
	        </LinearLayout>  
	   <LinearLayout
