  <LinearLayout
	  	    android:id="@+id/ButtonHistory"
	  	    android:layout_width="fill_parent"
	  	    android:layout_height="@dimen/historyDialogHeight"
	  	    android:gravity="center"
	  	    android:layout_gravity="center_vertical"
	  	    android:orientation="horizontal"
	  	    android:paddingTop="@dimen/standardPadding" >  
    
        <Button
        android:id="@+id/dialogButtonDelete"
        android:layout_width="@dimen/buttonWidth"
        android:layout_height="@dimen/buttonHeight"
        android:text="Delete"
        android:textSize="@dimen/historyDetailsSize"
        android:gravity="center"
        android:paddingLeft="@dimen/standardPadding"

        />
        
        <Button
        android:id="@+id/dialogButtonOK"
        android:layout_width="@dimen/buttonWidth"
        android:layout_height="@dimen/buttonHeight"
        android:text="OK"
        android:gravity="center"
        android:textSize="@dimen/historyDetailsSize"

       
        />
  </LinearLayout>  
   <LinearLayout
	  	    android:layout_width="fill_parent"
	  	    android:layout_height="@dimen/historyDetailsSize"
	  	    android:gravity="center"
